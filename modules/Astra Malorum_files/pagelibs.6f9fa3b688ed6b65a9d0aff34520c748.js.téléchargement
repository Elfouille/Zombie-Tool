(function() {

    var init = function() {

		initAnchorIntercept();

    };

    var initAnchorIntercept = function() {

		var pathname = window.location.href.split("#")[0];
        $('a').each(function() {
           	var link = $(this).attr('href');
           	if(typeof link !== "undefined") {
               if (link.substr(0,1) == "#") {
                   $(this).attr('href', pathname + link);
               }
            }
            else {
                //There is an empty <a> tag
            }
        });

    };

	$(init);

})();

// leaflet-registry.js
(function () {

    // Global registry for callbacks
    window.leafletReadyCallbacks = [];

    /**
     * Register a callback to run after Leaflet + plugins are ready
     * @param {Function} callback
     */
    window.onLeafletReady = function (callback) {
        if (window.leafletReady) {
            // Leaflet is already ready, run immediately
            callback(window.leafletInstance);
        } else {
            // Queue the callback
            window.leafletReadyCallbacks.push(callback);
        }
    };

    /**
     * Load CSS or JS dynamically
     * @param {string} url - Resource URL
     * @param {string} type - 'css' or 'js'
     * @returns {Promise}
     */
    function loadResource(url, type) {
        return new Promise((resolve, reject) => {
            let element;
            if (type === "css") {
                element = document.createElement("link");
                element.rel = "stylesheet";
                element.href = url;
            } else {
                element = document.createElement("script");
                element.src = url;
            }
            element.onload = resolve;
            element.onerror = () => reject(new Error(`Failed to load ${url}`));
            document.head.appendChild(element);
        });
    }

    /**
     * Load Leaflet and plugins dynamically
     * @param {Array} plugins - Array of { css: string, js: string }
     * @returns {Promise<object>} - Resolves with Leaflet namespace
     */
    function loadLeafletWithPlugins(plugins = []) {
        const leafletCSS = "https://unpkg.com/leaflet@1.9.4/dist/leaflet.css";
        const leafletJS = "https://unpkg.com/leaflet@1.9.4/dist/leaflet.js";

        const resources = [loadResource(leafletCSS, "css"), loadResource(leafletJS, "js")];

        // Add plugin resources
        plugins.forEach((plugin) => {
            if (plugin.css) resources.push(loadResource(plugin.css, "css"));
            if (plugin.js) resources.push(loadResource(plugin.js, "js"));
        });

        return Promise.all(resources).then(() => {
            if (typeof L !== "undefined" && typeof L.noConflict === "function") {
                return L.noConflict(); // Release global L
            } else {
                throw new Error("Leaflet failed to load.");
            }
        });
    }

    /**
     * Initialize Leaflet Registry
     */
    async function onDocumentReady() {

        // âŒ› Load Leaflet + plugins
        loadLeafletWithPlugins()
            .then((leaflet) => {
                // Mark Leaflet as ready and store instance
                window.leafletReady = true;
                window.leafletInstance = { leaflet };

                // Run all queued callbacks
                window.leafletReadyCallbacks.forEach((cb) => cb(window.leafletInstance));
                window.leafletReadyCallbacks = []; // Clear registry
            })
            .catch((err) => console.error(err));
    }

    document.readyState !== "loading" ? onDocumentReady() : document.addEventListener("DOMContentLoaded", onDocumentReady);
})();

